       RowBox[{"Pi", "/", "2"}]}], " ", "&&", " ", 
      RowBox[{"a2", " ", ">", " ", "0"}], "&&", " ", 
      RowBox[{"\[Epsilon]", " ", ">", " ", "0"}], "&&", "  ", 
      RowBox[{"\[Epsilon]", "\[NotEqual]", 
       RowBox[{"a2", " ", "q", " ", 
